---
import Swiper from '../components/Swiper.astro';
const { members } = Astro.props;

const memberSlides = members.map(member => (
  {
    image: `/imageFile/member_img/${member.key}.svg`,
    alt: member.name,
    key: member.key
  }
));

---

<Swiper slides={memberSlides} isNavigationBtn={true} />